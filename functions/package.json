{
  "name": "@blogify/functions",
  "version": "1.0.0",
  "description": "Blogify serverless functions",
  "main": "index.js",
  "scripts": {
    "dev": "serverless offline",
    "deploy": "serverless deploy",
    "deploy:dev": "npx ts-node scripts/deploy-api-and-log.ts",
    "deploy:prod": "npx ts-node scripts/deploy-to-prod-and-log.ts",
    "api-info": "npx ts-node scripts/log-api-info.ts",
    "remove": "serverless remove",
    "lint": "eslint . --ext .ts",
    "type-check": "tsc --noEmit",
    "docs:generate": "serverless openapi generate",
    "docs:serve": "npx swagger-ui-watcher openapi.yml",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "serverless",
    "aws",
    "lambda",
    "blogging",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.130",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.0",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "serverless-offline": "^13.3.0",
    "serverless-openapi-documentation": "^0.4.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.490.0",
    "@aws-sdk/client-dynamodb": "^3.490.0",
    "@aws-sdk/client-s3": "^3.490.0",
    "@aws-sdk/lib-dynamodb": "^3.490.0",
    "@aws-sdk/s3-request-presigner": "^3.490.0",
    "uuid": "^9.0.1"
  }
}

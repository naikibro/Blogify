{
  "name": "blogify",
  "version": "1.0.0",
  "description": "Headless blogging platform using AWS Lambda",
  "private": true,
  "workspaces": [
    "functions",
    "frontend",
    "documentation"
  ],
  "scripts": {
    "dev:docs": "yarn workspace docs start",
    "functions:deploy": "yarn workspace @blogify/functions deploy:dev",
    "functions:deploy:prod": "yarn workspace @blogify/functions deploy:prod",
    "functions:info": "yarn workspace @blogify/functions info",
    "docs": "yarn workspace docs build && yarn workspace docs start",
    "frontend": "yarn workspace @blogify/frontend dev",
    "lint": "yarn workspaces run lint"
  },
  "keywords": [
    "serverless",
    "aws",
    "lambda",
    "blogging",
    "typescript",
    "nextjs"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "yarn@1.22.0"
}
